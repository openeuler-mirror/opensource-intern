# Registry details
unset OCI_ROOT_URL
unset OCI_NAMESPACE
unset OCI_CROSSMOUNT_NAMESPACE
unset OCI_USERNAME
unset OCI_PASSWORD

# Which workflows to run
unset OCI_TEST_PULL
unset OCI_TEST_PUSH
unset OCI_TEST_CONTENT_DISCOVERY
unset OCI_TEST_CONTENT_MANAGEMENT

# Extra settings
unset OCI_HIDE_SKIPPED_WORKFLOWS
unset OCI_DEBUG
unset OCI_DELETE_MANIFEST_BEFORE_BLOBS
unset OCI_AUTOMATIC_CROSSMOUNT
